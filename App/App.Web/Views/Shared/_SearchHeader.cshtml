<div class="card card-custom gutter-b">
    <div class="card-header">
        <div class="card-title">
            <span class="card-icon">
                <i class="flaticon-search text-primary"></i>
            </span>
            <h3 class="card-label">
                Search
            </h3>
        </div>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-xl-6">
                <div class="form-group row">
                    <div class="input-daterange input-group" id="kt_datepicker_range">
                        <label class="col-xl-2 col-form-label text-right">From</label>
                        <div class="col-4">
                            <input type="text" class="form-control" name="start" placeholder="dd/mm/yyyy" id="from_date" autocomplete="off" />
                        </div>

                        <label class="col-1 col-form-label text-right">To</label>
                        <div class="col-4">
                            <input type="text" class="form-control" name="end" placeholder="dd/mm/yyyy" id="to_date" autocomplete="off" />
                        </div> 
                    </div>

                    </div>
            </div>
            <div class="col-xl-2">
                <div class="form-group mb-0">
                    <button class="btn btn-primary font-weight-bolder" type="button" onclick="SearchByDate()">
                        <i class="fa fa-search"></i> Search
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

